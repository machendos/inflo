{
  "tockens": {
    
    "DELIM": "[(){},[\\]]",
    "IDENTIFIER": "[A-Za-z_][A-Za-z_0-9?-]*",
    "STRING": "'[A-Za-z0-9!@#$%^&?.*()_\\-+=\\/~\\ |[\\]{}]*'",
    "KEY_WORD": ["import", "fun"],
    "NUMBER": "\\-?((0|[1-9][0-9])*\\.|[1-9])[0-9]*",
    "OPEN_STATEMENT": ["\\("],
    "CLOSE_STATEMENT": ["\\)"],
    "IMPORT": ["import"],
    "DECLARATION": ["fun"],

    "LIST_START": ["\\{"],
    "LIST_END": ["\\}"],

    "BOOLEAN": ["T", "F"],

    "COMMENTS": ["//[A-Za-z0'-9!@#$%^&?.*()_\\-+=\\/~\\|[\\] {}]*\n"]

  },

  "configs": { 
    "spaces_process": "none",
    "on_error": "continue"
  }

}